{
  "verbose": true,
  "preset": "ts-jest/presets/default-esm",
  "collectCoverage": true,
  "collectCoverageFrom": ["source/**/*.ts"],
  "testTimeout": 30000,
  "testMatch": ["test/*\\.test\\.ts$"],
  "moduleFileExtensions": ["js", "json", "ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
