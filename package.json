{
  "name": "node-rate-limit",
  "version": "0.0.0",
  "description": "A rate limiting library for Node, using Redis. Implements 5 different rate limiting algorithms.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">= 22"
  },
  "homepage": "https://github.com/whiteboardclub/node-rate-limit",
  "repository": "https://github.com/whiteboardclub/node-rate-limit",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "tsconfig.json",
    "package.json",
    "readme.md",
    "license.md",
    "changelog.md"
  ],
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "pkgroll --src source/",
    "prepare": "husky install",
    "lint": "eslint --ignore-path .gitignore \"{src,tests}/**/*.+(ts|js|tsx)\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json|tsx|mdx)\" --log-level silent"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{ts,js,md}": "prettier --write",
    "*.js": "eslint --cache --fix",
    "*.{js,md}": "prettier --write"
  },
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "pkgroll": "^2.6.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
