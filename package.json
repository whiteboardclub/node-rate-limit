{
  "name": "node-rate-limit",
  "version": "0.0.0",
  "description": "A rate limiting library for Node, using Redis. Implements 5 different rate limiting algorithms.",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">= 22"
  },
  "homepage": "https://github.com/whiteboardclub/node-rate-limit",
  "repository": "https://github.com/whiteboardclub/node-rate-limit",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "tsconfig.json",
    "package.json",
    "readme.md",
    "license.md",
    "changelog.md"
  ],
  "scripts": {
    "test": "jest --config jest.config.json",
    "build": "pkgroll --src src/"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "pkgroll": "^2.6.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  }
}
